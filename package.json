{
  "name": "vue3-q-tel-input",
  "version": "1.0.53",
  "description": "Vue 3 telephone input implemented over Quasar framework",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "rm -rf dist && cross-env NODE_ENV=production rollup --config build/rollup.config.js && rm -rf dist/component",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "lint": "eslint src --ext .ts,.vue"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CdTgr/vue3-q-tel-input.git"
  },
  "homepage": "https://github.com/CdTgr/vue3-q-tel-input/blob/master/readme.md",
  "keywords": [
    "vue3",
    "vue-3",
    "vue 3",
    "vue3-tel-input",
    "phone input",
    "telephone input",
    "international phone",
    "quasar",
    "quasar v2",
    "quasar framework",
    "quasar framweork v3",
    "country",
    "input",
    "country input",
    "telephone",
    "tel input",
    "telephon input",
    "international number",
    "international",
    "phone number",
    "phone",
    "q-tel",
    "q-tel-input",
    "q tel",
    "q tel input",
    "quasar telephone",
    "quasar telephone input",
    "quasar phone number",
    "quasar phone",
    "quasar phone number input",
    "vue phone number",
    "vue phone input",
    "vue phone international"
  ],
  "main": "dist/vue3-q-tel-input.ssr.js",
  "module": "dist/vue3-q-tel-input.esm.js",
  "browser": "dist/vue3-q-tel-input.esm.js",
  "unpkg": "dist/vue3-q-tel-input.min.js",
  "files": [
    "dist/*",
    "vue3-q-tel-input.d.ts"
  ],
  "dependencies": {
    "libphonenumber-js": "^1.10.20"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@quasar/extras": "^1.0.0",
