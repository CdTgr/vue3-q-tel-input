{
  "name": "vue3-q-tel-input",
  "version": "1.1.9",
  "description": "Vue 3 telephone input implemented over Quasar framework",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "test": "exit 0",
    "build": "rm -rf dist && cross-env NODE_ENV=production rollup --config build/rollup.config.mjs && rm -rf dist/component",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.mjs --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.mjs --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.mjs --format iife",
    "lint": "eslint --fix --ignore-pattern .gitignore  src/",
    "format": "prettier --write src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CdTgr/vue3-q-tel-input.git"
  },
  "homepage": "https://github.com/CdTgr/vue3-q-tel-input/blob/master/readme.md",
  "keywords": [
    "vue3",
    "vue-3",
    "vue 3",
    "vue3-tel-input",
    "phone input",
    "telephone input",
    "international phone",
    "quasar",
    "quasar v2",
    "quasar framework",
    "quasar framweork v3",
    "country",
    "input",
    "country input",
    "telephone",
    "tel input",
    "telephon input",
    "international number",
    "international",
    "phone number",
    "phone",
    "q-tel",
    "q-tel-input",
    "q tel",
    "q tel input",
    "quasar telephone",
    "quasar telephone input",
