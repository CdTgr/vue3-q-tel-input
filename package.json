  "main": "dist/vue3-q-tel-input.ssr.js",
  "module": "dist/vue3-q-tel-input.esm.js",
  "browser": "dist/vue3-q-tel-input.esm.js",
  "unpkg": "dist/vue3-q-tel-input.min.js",
  "files": [
    "dist/*",
    "vue3-q-tel-input.d.ts"
  ],
  "dependencies": {
    "libphonenumber-js": "^1.11.11"
  },
  "devDependencies": {
