    "sass": "1.32.12",
    "sass-loader": "^10.1.0",
    "typescript": "^4.3.5",
    "vue": "^3.0.5",
    "vue-cli-plugin-quasar": "~4.0.1",
    "vue-eslint-parser": "^9.1.0"
  },
  "peerDependencies": {
    "quasar": "^2.0.0",
